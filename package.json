{
    "name": "express-template",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run build && npm run lint && node ./dist/bin/server.js",
        "dev": "ts-node-dev --no-notify --respawn --transpile-only src/bin/server.ts",
        "clean": "rm -rf dist",
        "build": "npm -s run clean && tsc ",
        "lint": "tsc --noEmit && eslint \"src/**/*.ts\" --quiet --fix",
        "prisma:init": "npx prisma migrate dev --name init",
        "test": "jest -i"
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "body-parser": "^1.20.1",
        "class-validator": "^0.14.0",
        "cookie-parser": "~1.4.6",
        "cors": "^2.8.5",
        "debug": "~4.3.4",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "helmet": "^6.0.1",
        "http-errors": "~2.0.0",
        "jsonwebtoken": "^9.0.0",
        "morgan": "~1.10.0",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.1",
        "pg": "^8.9.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cookie-parser": "~1.4.3",
        "@types/cors": "^2.8.13",
        "@types/debug": "~4.1.7",
        "@types/express": "^4.17.17",
        "@types/express-rate-limit": "^5.1.3",
        "@types/http-errors": "~2.0.1",
        "@types/jest": "^29.4.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/morgan": "~1.9.4",
        "@types/node": "^18.13.0",
        "@types/nodemailer": "^6.4.7",
        "@types/pg": "^8.6.6",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.4.2",
        "jest-mock-extended": "^3.0.1",
        "prettier": "^2.8.4",
        "prisma": "^4.10.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    }
}
