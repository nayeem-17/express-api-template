{
    "name": "express-template",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run build && npm run lint && node ./dist/bin/server.js",
        "dev": "ts-node-dev --no-notify --respawn --transpile-only src/bin/server.ts",
        "clean": "rm -rf dist",
        "build": "npm -s run clean && tsc -p .",
        "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix",
        "prisma:init": "npx prisma migrate dev --name init"
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "body-parser": "^1.19.0",
        "class-validator": "^0.14.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "^4.18.2",
        "express-rate-limit": "^5.2.3",
        "helmet": "^4.4.1",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.10.13",
        "morgan": "~1.9.1",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.5.0",
        "oracledb": "^5.3.0",
        "pg": "^8.6.0",
        "swagger-jsdoc": "^6.0.2",
        "swagger-ui-express": "^4.1.6",
        "tsoa": "^5.1.1",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cookie-parser": "~1.4.2",
        "@types/cors": "^2.8.12",
        "@types/debug": "~4.1.7",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/http-errors": "~1.6.3",
        "@types/jest": "^27.4.0",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/morgan": "~1.9.3",
        "@types/node": "^17.0.45",
        "@types/nodemailer": "^6.4.4",
        "@types/oracledb": "^5.2.2",
        "@types/pg": "^8.6.3",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.9.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "prettier": "^2.3.2",
        "prisma": "^4.10.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.9.5"
    }
}
